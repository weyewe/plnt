<h1>Listing engine_brands</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @engine_brands.each do |engine_brand| %>
  <tr>
    <td><%= engine_brand.name %></td>
    <td><%= link_to 'Show', engine_brand %></td>
    <td><%= link_to 'Edit', edit_engine_brand_path(engine_brand) %></td>
    <td><%= link_to 'Destroy', engine_brand, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

